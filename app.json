{
  "expo": {
    "name": "MyCalorie",
    "slug": "MyCalorie",
    "scheme": "mycalorie",
    "deepLinking": true,
    "version": "1.0.0",
    "sdkVersion": "50.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#D8D8F6"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#ffffff",
      "hidden": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ayiqazmi.MyCalorie"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
